#include "stdafx.h"
#include "stdlib.h"

int main()
{
	char arr[3][5] = {};
	int i = 0; 
	int j = 0;
	int m = 0;
	int n = 0;

	for (i = 0; i < 3; i++)
	{
		for (j = 0; j < 5; j++)
		{
			scanf(" %c", &arr[i][j]);
		}
	}


	for (m = 0; m < 3; m++)
	{
		for (n = 0; n < 5; n++)
		{
			printf("%c ", arr[m][n] + 32);
		}
		printf("\n");
	}





	system("pause");
	return 0;
}
