#include "stdafx.h"
#include "stdlib.h"


int _tmain(int argc, _TCHAR* argv[])
{
  int a=0;
  int b=1;
  int c;
  int d;
  
  printf("몇번째 항까지 구할까요 : "); 
  scanf("%d",&d); printf("%d %d", a, b);
  
  for(int x=1;x<d;x++) 
  {
    c=a+b;  
    printf("%d", c);  
    a=b;  b=c; 
  }  
  printf("\n"); 
  
  system("pause"); 
  return 0;
}
