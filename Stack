#include "stdafx.h"
#include "stdlib.h"
#define MAX 20
char stack[MAX];
int top = -1;
char a[20];

void push(char x)
{
    if(top>=MAX-1)
        printf("Stack Overflow");
        else
        {
             top=top+1;
             stack[top]=x;
        }
}
void pop()
{
    if(top<0)
        printf("No Stack");
    else
    {
        printf("%c\n", stack[top]);
        top=top-1;
    }
}
int main()
{
    scanf_s("%20s", a, 19);
    for(int i=0;a[i];i++)
    {
        push(a[i]);
    }
    pop();
    
    system("pause");
    return 0;
}
